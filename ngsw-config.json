{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/",
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "api",
      "installMode": "prefetch",
      "resources": {
        "urls": [
          "https://pokeapi.co/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "POKEMON API",
      "urls": [
        "https://pokeapi.co/**"
      ],
      "version": 1,
      "cacheConfig": {
        "maxSize": 10000,
        "maxAge": "1d",
        "strategy": "freshness"
      },
      "cacheQueryOptions": {
        "ignoreSearch": false
      }
    }
  ]
}